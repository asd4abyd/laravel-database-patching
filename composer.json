{
  "name": "dweik/laravel-database-patching",
  "description": "Laravel database patching",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "1.0.0",
  "keywords": [
    "laravel", "database", "patching", "migration", "seed", "seeding", "schema",
    "command", "artisan", "incremantal", "inserting", "updating", "deleting",
    "insert", "update", "delete", "adding", "modifying", "add", "modify"
  ],
  "authors": [
